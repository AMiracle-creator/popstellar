{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "resource:/protocol/qrcode/identity.json",
  "title": "QRCode data for the user's main public key",
  "description": "Match a data contained in a qrcode encoding the main public key",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "identity": {
      "type": "string",
      "contentEncoding": "base64",
      "description": "The main public key"
    }
  },
  "required": ["identity"]
}