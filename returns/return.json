{
    "$comment": "all requests except for broadcast and return expect a return as an ack",
    "$schema": "http://json-schema.org/draft/2019-09/schema#",
    "$id": "http://pop2020.dedis.ch/returns/return.json",
    "type": "object",
    "properties" : {
        "oneOf": [
            {
                "result": {
                    "description": "For responses, present on success, absent on error",
                    "type": "integer"
                }
            },
            {
                "error": {
                    "$ref": "errors/error.json#/definition"
                }
            }
        ]

    },
    "required": ["oneOf"]
}
